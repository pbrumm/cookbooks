client
dev tun
proto <%= node["openvpn"]["proto"] %>
remote <%= node["openvpn"]["gateway"] %> <%= node["openvpn"]["port"] %>
resolv-retry infinite
nobind
persist-key
persist-tun
ca <%= node["openvpn"]["client_key_dir"] %>/ca.crt
cert <%= node["openvpn"]["client_key_dir"] %>/<%= node["openvpn"]["client_name"] %>.crt
key <%= node["openvpn"]["client_key_dir"] %>/<%= node["openvpn"]["client_name"] %>.key
comp-lzo
verb 3
